/*
Author : Bharathi H
Purpose :Example given number is even or odd
*/


#include <stdio.h>

int main()
{
   int number,remainder;
   
   printf("Enter the number: ");
   scanf("%d",&number);
   
   remainder = number%2;
   
   if(remainder == 0)
   {
   printf("number is even");
   }
   else
   {
   printf("number is odd");
   }
   
   return 0;

   
}
