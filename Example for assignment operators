/*
Author : Bharathi H
Purpose :Example for assignment operators
*/

#include<stdio.h>
int main ( )
{
  int a = 15;
  int c = 33;
  
  c += a;    									
  printf("c is %d\n",c);
  
  c -= a;   								
  printf("c is %d\n",c);
  
  c *= a;   									
  printf("c is %d\n",c);
  
  c /= a;  									
  printf("c is %d\n",c);
  
  c %= a;  									
  printf("c is %d\n",c);
   
  return 0;
}
